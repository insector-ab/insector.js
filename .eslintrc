{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [],
    "extends": "standard",
    "rules": {
        "indent": [2, 4, {"SwitchCase": 1}],
        "semi": [2, "always"],
        "space-before-function-paren": [2, "never"],
        "padded-blocks": [2, "never"]
    }
}
