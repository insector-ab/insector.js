{
  "name": "insector.js",
  "description": "Stuff for javascript projects.",
  "version": "0.1.3",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npm run build-form & npm run build-node-model & npm run build-react-components & npm run build-react-mvc & npm run build-utils & npm run build-xhr-service",
    "watch": "npm run watch-form & npm run watch-node-model & npm run watch-react-components & npm run watch-react-mvc & npm run watch-utils & npm run watch-xhr-service",
    "build-form": "babel packages/insector-form/src --out-dir packages/insector-form/lib",
    "watch-form": "babel packages/insector-form/src --watch --out-dir packages/insector-form/lib",
    "build-node-model": "babel packages/insector-node-model/src --out-dir packages/insector-node-model/lib",
    "watch-node-model": "babel packages/insector-node-model/src --watch --out-dir packages/insector-node-model/lib",
    "build-react-components": "babel packages/insector-react-components/src --out-dir packages/insector-react-components/lib",
    "watch-react-components": "babel packages/insector-react-components/src --watch --out-dir packages/insector-react-components/lib",
    "build-react-mvc": "babel packages/insector-react-mvc/src --out-dir packages/insector-react-mvc/lib",
    "watch-react-mvc": "babel packages/insector-react-mvc/src --watch --out-dir packages/insector-react-mvc/lib",
    "build-utils": "babel packages/insector-utils/src --out-dir packages/insector-utils/lib",
    "watch-utils": "babel packages/insector-utils/src --watch --out-dir packages/insector-utils/lib",
    "build-xhr-service": "babel packages/insector-xhr-service/src --out-dir packages/insector-xhr-service/lib",
    "watch-xhr-service": "babel packages/insector-xhr-service/src --watch --out-dir packages/insector-xhr-service/lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint packages/*/src/**.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insector-ab/insector.js.git"
  },
  "author": "Insector AB <mail@insector.se>",
  "bugs": {
    "url": "https://github.com/insector-ab/insector.js/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1"
  }
}
